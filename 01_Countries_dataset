# Data Manipulation with dplyr in R 
# data: 2015 United States Census  

select() 
filter() 
arrange() 
mutate()

countries
glimpse(countries)

# select
countries %>%   # %>%: pipe operator
  select(state, country, population, unemployment)
  
# creating a new table
countries_selected <- countries %>%    
  select(state, country, population, unemployment)

countries_selected 
  
